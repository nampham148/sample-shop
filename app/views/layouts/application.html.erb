<!DOCTYPE html>
<html>
  <head>
    <title><%= yield(:title) %> </title>
    <%= render 'layouts/config' %>
  </head>

  <body>
  	<%= render 'layouts/header' %>
    <div class="container"> 
    	<% flash.each do |message_type, message| %>
    		<%= content_tag(:div, message, class: "alert alert-#{message_type}") %>
    	<% end %>
    	<%= yield %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>
